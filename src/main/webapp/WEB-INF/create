CREATE TABLE BOARD_INFO(
BI_NUM INT  AUTO_INCREMENT PRIMARY KEY,
BI_TITLE VARCHAR(100) NOT NULL,
BI_CONTENT TEXT NOT NULL,
BI_WRITER VARCHAR(30) NOT NULL,
BI_CREDAT TIMESTAMP NOT NULL,
BI_CNT INT NOT NULL
);
SELECT NOW();

INSERT INTO BOARD_INFO(BI_TITLE,BI_CONTENT,BI_WRITER,
BI_CREDAT,BI_CNT)
VALUES('첫번째 게시물','냉무','글쓴이',NOW(),0);

SELECT *FROM BOARD_INFO;
